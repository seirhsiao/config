            "icons": [
                "http://ox.xmkczs.com/quantumultX/xmbs.png",
                "http://ox.xmkczs.com/quantumultX/xmbs.png"
            ],
            "keys": [
                "XIAOMI_AMAZFIT_USERNAME",
                "XIAOMI_AMAZFIT_PASSWORD",
                "XIAOMI_AMAZFIT_LOGIN_TOKEN",
                "STED_SIZE_RANGE",
                "IS_CLOUD_API"
