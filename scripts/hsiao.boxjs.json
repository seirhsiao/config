                {
                    "id": "XIAOMI_AMAZFIT_PASSWORD",
                    "name": "密码：",
                    "val": "",
                    "type": "text",
                    "placeholder": "小米运动密码",
                    "desc": "多个用英文“#”隔开"
                },
                {
                    "id": "STED_SIZE_RANGE",
                    "name": "步数：",
                    "val": "",
                    "type": "text",
                    "placeholder": "固定步数或随机步数",
