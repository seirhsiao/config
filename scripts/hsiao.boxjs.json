            "settings": [
                {
                    "id": "XIAOMI_AMAZFIT_USERNAME",
                    "name": "账号：",
                    "val": "",
                    "type": "text",
                    "placeholder": "小米运动账号",
                    "desc": "多个用英文“#”隔开"
